# Environment Configuration Example
# Copy to .env and modify as needed

# Security
SECRET_KEY=your-super-secret-key-change-in-production
REQUIRE_AUTH=false
AUTH_USERNAME=admin
AUTH_PASSWORD=password

# Performance
CAMERA_WIDTH=1280
CAMERA_HEIGHT=720
TARGET_FPS=30
MAX_HISTORY_SIZE=100

# Paths
MODEL_PATH=models/accessibility_gesture_model.h5
TRAINING_DATA_PATH=training_data/training_samples.pkl
SETTINGS_PATH=settings.json

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/accessibility_system.log
MAX_LOG_SIZE=10485760
LOG_BACKUPS=5